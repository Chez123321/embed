#include "embed.hpp"
#include <string>
#include <unordered_map>

@EXTERNS@

namespace embed {
    static std::unordered_map<std::string, File> map = @MAP@

    File get(const std::string& path) {
        auto it = map.find(path);
        if (it != map.end()) {
            return it->second;
        }
        return File(nullptr, nullptr);
    }
}
